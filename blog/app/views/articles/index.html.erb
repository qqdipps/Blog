<h1>Table of Contents</h1>
<%= link_to 'New Entry', new_article_path %>
<table>
	<tr>
		<th>Title</th>
		<th>Text</th>
		<th></th>
	</tr>

	<% @articles.each do |article| %>
	<tr>
		<td> <%= article.title %> </td>
		<td> <%= article.text %> </td>
		<td> <%= link_to 'Show', article_path(article) %></td>
	</tr>
	<% end %>
</table>

<%= link_to 'Back', welcome_index_path %>
